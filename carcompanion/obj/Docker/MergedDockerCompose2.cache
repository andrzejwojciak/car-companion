services:
  carcompanion:
    build:
      context: /Users/mateuszlesniara/Desktop/carbackend/carcompanionbackend/carcompanion
      dockerfile: carcompanion/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: carcompanion
      target: base
    container_name: carcompanion
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: ''
    image: carcompanion:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: '  "/app/bin/Debug/netcoreapp3.1/carcompanion.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - target: 80
    - target: 443
    tty: true
    volumes:
    - /Users/mateuszlesniara/Desktop/carbackend/carcompanionbackend/carcompanion/carcompanion:/app:rw
    - /Users/mateuszlesniara/vsdbg/vs2017u5:/remote_debugger:rw
    - /Users/mateuszlesniara/.aspnet/https:/root/.aspnet/https:ro
    - /Users/mateuszlesniara/.microsoft/usersecrets:/root/.microsoft/usersecrets:ro
    - /Users/mateuszlesniara/Desktop/carbackend/carcompanionbackend/carcompanion:/src:rw
  mssql:
    environment:
      ACCEPT_EULA: "Y"
      SA_PASSWORD: Password123
    image: microsoft/mssql-server-linux
    ports:
    - published: 14433
      target: 14433
version: '3.4'